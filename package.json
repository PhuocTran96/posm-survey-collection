{
  "name": "survey-posm",
  "version": "1.0.0",
  "description": "POSM Survey System",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "upload-posm": "node upload-posm-data.js",
    "upload-posm-clear": "node upload-posm-data.js --clear",
    "upload-posm-insert": "node upload-posm-data.js --insert-only",
    "upload-posm-update": "node upload-posm-data.js --update-only",
    "upload-posm-upsert": "node upload-posm-data.js --upsert",
    "import-users": "node import-users.js",
    "create-super-admin": "node import-users.js --create-super-admin",
    "lint": "eslint src --ext .js --max-warnings=0",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build:posm-matrix": "webpack --config webpack.config.js --mode production",
    "build:posm-matrix:dev": "webpack --config webpack.config.js --mode development --watch"
  },
  "dependencies": {
    "ag-grid-community": "^31.0.0",
    "ag-grid-react": "^31.0.0",
    "aws-sdk": "^2.1531.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^7.5.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.32.6",
    "xlsx": "^0.18.5"
