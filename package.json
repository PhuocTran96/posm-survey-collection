  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.23.0",
    "@eslint/js": "^9.34.0",
    "@tailwindcss/postcss": "^4.1.13",
    "autoprefixer": "^10.4.21",
    "axios": "^1.11.0",
    "babel-loader": "^9.1.0",
    "css-loader": "^6.8.0",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.3.0",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.9.4",
    "nodemon": "^3.0.1",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.2.0",
    "puppeteer": "^24.17.1",
    "style-loader": "^3.3.0",
    "tailwindcss": "^4.1.13",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.0"
